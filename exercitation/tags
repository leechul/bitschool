mMath	student.h	/^    int     mMath;$/;"	m	struct:student
mName	student.h	/^    char    mName[20];$/;"	m	struct:student
mNum	student.h	/^    int     mNum;$/;"	m	struct:student
mSum	student.h	/^    int     mSum;$/;"	m	struct:student
main	main.c	/^int main ()$/;"	f
print_input_message	student.c	/^void print_input_message()$/;"	f
print_line	student.c	/^void print_line()$/;"	f
print_output_message	student.c	/^void print_output_message()$/;"	f
