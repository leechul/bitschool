SOURCES	Makefile	/^SOURCES=main.c student.c$/;"	m
Student	student.h	/^}Student;$/;"	t	typeref:struct:student
average	student.c	/^int average(int a, int b, int c)$/;"	f
getInfo	main.c	/^int getInfo()$/;"	f
getStudent	student.c	/^int getStudent()$/;"	f
level	student.c	/^char level( int a )$/;"	f
mAverage	student.h	/^    int     mAverage;$/;"	m	struct:student
